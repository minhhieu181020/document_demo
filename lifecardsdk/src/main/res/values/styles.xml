<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LifeCardSDK_AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/lifecardsdk_colorPrimary</item>
        <item name="colorPrimaryDark">@color/lifecardsdk_colorPrimaryDark</item>
        <item name="colorAccent">@color/lifecardsdk_colorAccent</item>
        <item name="colorControlNormal">@color/lifecardsdk_black_effective</item>
        <item name="android:windowBackground">@color/lifecardsdk_grey_five</item>
    </style>

    <style name="LifeCardSDK_AppThemeFullScreen" parent="LifeCardSDK_AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="LifeCardSDK_PrimaryGray" parent="LifeCardSDK_AppThemeFullScreen">
        <item name="colorPrimary">@color/lifecardsdk_defaut_toolbar_color</item>
    </style>

    <style name="LifeCardSDK_AppBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="colorAccent">@color/lifecardsdk_colorAccent</item>
        <item name="bottomSheetStyle">@style/LifeCardSDK_AppModalStyle</item>
    </style>

    <style name="LifeCardSDK_AppModalStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@drawable/lifecardsdk_rounded_bottom_sheet</item>
    </style>

    <style name="LifeCardSDK_SquareMenuItem">
        <item name="android:foreground">?selectableItemBackground</item>
    </style>

    <style name="LifeCardSDK_RoundMenuItem">
        <item name="android:background">?actionBarItemBackground</item>
    </style>

    <style name="LifeCardSDK_NormalText">
        <item name="android:textSize">@dimen/lifecardsdk_sp_15</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_NormalText_Orange">
        <item name="android:textSize">@dimen/lifecardsdk_sp_15</item>
        <item name="android:textColor">@color/lifecardsdk_orange</item>
    </style>

    <style name="LifeCardSDK_NormalText_14">
        <item name="android:textSize">@dimen/lifecardsdk_sp_14</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_NormalText_14_Gray">
        <item name="android:textSize">@dimen/lifecardsdk_sp_14</item>
        <item name="android:textColor">@color/lifecardsdk_gray1</item>
    </style>

    <style name="LifeCardSDK_NormalText_Gray">
        <item name="android:textSize">@dimen/lifecardsdk_sp_15</item>
        <item name="android:textColor">@color/lifecardsdk_gray1</item>
    </style>

    <style name="LifeCardSDK_TabLayout" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">@dimen/lifecardsdk_sp_16</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="LifeCardSDK_SmallText">
        <item name="android:textSize">@dimen/lifecardsdk_sp_11</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_SmallText_13sp">
        <item name="android:textSize">@dimen/lifecardsdk_sp_13</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_NormalTextBold" parent="LifeCardSDK_NormalText">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="LifeCardSDK_Text_18_Bold">
        <item name="android:textSize">@dimen/lifecardsdk_sp_19</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_HeaderText">
        <item name="android:textSize">@dimen/lifecardsdk_sp_18</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_TitleText">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_16</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_HeaderText_NoBold" parent="LifeCardSDK_HeaderText">
        <item name="android:textStyle">normal</item>
    </style>

    <style name="LifeCardSDK_HeaderTextBold" parent="LifeCardSDK_HeaderText">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="LifeCardSDK_BigText">
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_20</item>
    </style>

    <style name="LifeCardSDK_BigText_Orange">
        <item name="android:textColor">@color/lifecardsdk_orange</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_28</item>
    </style>

    <style name="LifeCardSDK_HeaderTextBold_20" parent="LifeCardSDK_HeaderText">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_20</item>
    </style>

    <style name="LifeCardSDK_HeaderText_sp24">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_24</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_HeaderText_sp20">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_20</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_HeaderText_sp18_maginTop8dp">
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">@dimen/lifecardsdk_dp_8</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_18</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>


    <style name="LifeCardSDK_ScreenTitle">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_19</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
    </style>

    <style name="LifeCardSDK_IconToolbar" parent="LifeCardSDK_RoundMenuItem">
        <item name="android:layout_width">@dimen/lifecardsdk_dp_42</item>
        <item name="android:layout_height">@dimen/lifecardsdk_dp_42</item>
        <item name="android:padding">@dimen/lifecardsdk_dp_4</item>
    </style>

    <style name="LifeCardSDK_IconToolbar_Big" parent="LifeCardSDK_SquareMenuItem">
        <item name="android:layout_width">@dimen/lifecardsdk_dp_48</item>
        <item name="android:layout_height">@dimen/lifecardsdk_dp_48</item>
        <item name="android:padding">@dimen/lifecardsdk_dp_4</item>
    </style>

    <style name="LifeCardSDK_Button_Small" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_14</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingLeft">@dimen/lifecardsdk_dp_10</item>
        <item name="android:paddingRight">@dimen/lifecardsdk_dp_10</item>
        <item name="android:paddingTop">@dimen/lifecardsdk_dp_8</item>
        <item name="android:paddingBottom">@dimen/lifecardsdk_dp_8</item>
    </style>

    <style name="LifeCardSDK_Button_Big" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:textAllCaps">false</item>
        <item name="android:foreground">?selectableItemBackground</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_13</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingLeft">@dimen/lifecardsdk_dp_16</item>
        <item name="android:paddingRight">@dimen/lifecardsdk_dp_16</item>
        <item name="android:paddingTop">@dimen/lifecardsdk_dp_10</item>
        <item name="android:paddingBottom">@dimen/lifecardsdk_dp_10</item>
    </style>

    <style name="LifeCardSDK_TextFilter">
        <item name="android:textSize">@dimen/lifecardsdk_sp_13</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingLeft">@dimen/lifecardsdk_dp_10</item>
        <item name="android:paddingRight">@dimen/lifecardsdk_dp_10</item>
        <item name="android:paddingTop">@dimen/lifecardsdk_dp_8</item>
        <item name="android:paddingBottom">@dimen/lifecardsdk_dp_8</item>
    </style>

    <style name="LifeCardSDK_MarginHorizontal">
        <item name="android:layout_marginLeft">@dimen/lifecardsdk_dp_8</item>
        <item name="android:layout_marginRight">@dimen/lifecardsdk_dp_8</item>
    </style>

    <style name="LifeCardSDK_PaddingHorizontal">
        <item name="android:paddingLeft">@dimen/lifecardsdk_dp_8</item>
        <item name="android:paddingRight">@dimen/lifecardsdk_dp_8</item>
    </style>


    <style name="LifeCardSDK_EditText">
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
        <item name="android:textColorHint">@color/lifecardsdk_hint_text</item>
        <item name="android:paddingTop">@dimen/lifecardsdk_dp_8</item>
        <item name="android:paddingBottom">@dimen/lifecardsdk_dp_8</item>
        <item name="android:paddingLeft">@dimen/lifecardsdk_dp_10</item>
        <item name="android:paddingRight">@dimen/lifecardsdk_dp_10</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_15</item>
    </style>

    <style name="LifeCardSDK_EditText.Drawable" parent="LifeCardSDK_EditText">
        <item name="android:drawablePadding">@dimen/lifecardsdk_dp_10</item>
        <item name="android:lines">1</item>
    </style>

    <style name="LifeCardSDK_Zoom_Center">
        <item name="android:windowEnterAnimation">@anim/lifecardsdk_zoom_center</item>
    </style>

    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">@dimen/lifecardsdk_sp_15</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="LifeCardMerchant_Zoom_Center">
        <item name="android:windowEnterAnimation">@anim/lifecardsdk__zoom_center</item>
    </style>

    <style name="lifecardsdk_Text15Bold">
        <item name="android:textSize">@dimen/lifecardsdk_sp_15</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="LifeCardSDK_ViewLine_Gray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/lifecardsdk_gray</item>
    </style>
    <style name="LifeCardSDK_ViewLine_Gray_Height">
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/lifecardsdk_gray</item>
    </style>

    <style name="LifeCardSDK_Background_View_White">
        <item name="android:paddingRight">@dimen/lifecardsdk_dp_12</item>
        <item name="android:paddingLeft">@dimen/lifecardsdk_dp_12</item>
        <item name="android:paddingTop">@dimen/lifecardsdk_dp_10</item>
        <item name="android:paddingBottom">@dimen/lifecardsdk_dp_12</item>
        <item name="android:background">@drawable/lifecardsdk_bg_round_white</item>
        <item name="android:elevation">@dimen/lifecardsdk_dp_2</item>
    </style>

    <style name="LifeCardSDK_Background_View_White.Click">
        <item name="android:foreground">?selectableItemBackground</item>
    </style>

    <style name="LifeCardSDK_Background_Status_Card">
        <item name="android:paddingLeft">@dimen/lifecardsdk_dp_12</item>
        <item name="android:paddingRight">@dimen/lifecardsdk_dp_12</item>
        <item name="android:paddingBottom">@dimen/lifecardsdk_dp_4</item>
        <item name="android:paddingTop">@dimen/lifecardsdk_dp_4</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_14</item>
    </style>

    <style name="LifeCardSDK_Text_Content_Left">
        <item name="android:layout_width">0dp</item>
        <item name="android:gravity">start</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_15</item>
        <item name="android:textColor">@color/lifecardsdk_gray1</item>
    </style>

    <style name="LifeCardSDK_Text_Content_Right">
        <item name="android:gravity">end</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/lifecardsdk_sp_15</item>
        <item name="android:textColor">@color/lifecardsdk_black_effective</item>
        <item name="android:layout_marginLeft">@dimen/lifecardsdk_dp_12</item>
    </style>


    <style name="LifeCardSDK_Card_Wait_IMG" >
        <item name="android:layout_width">@dimen/lifecardsdk_dp_16</item>
        <item name="android:layout_height">@dimen/lifecardsdk_dp_16</item>
        <item name="android:layout_marginTop">@dimen/lifecardsdk_dp_4</item>
    </style>
    <style name="LifeCardSDK_Card_Wait_Text" parent="LifeCardSDK_NormalText" >
        <item name="android:layout_marginLeft">@dimen/lifecardsdk_dp_8</item>
        <item name="android:textSize">@dimen/lifecardsdk_dp_16</item>
    </style>

    <style name="LifeCardSDKTextViewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColorHint">@color/lifecardsdk_text_color_77</item>
        <item name="android:textColor">@color/lifecardsdk_text_color_42</item>
        <item name="android:textSize">@dimen/_12_sdp</item>
    </style>

</resources>